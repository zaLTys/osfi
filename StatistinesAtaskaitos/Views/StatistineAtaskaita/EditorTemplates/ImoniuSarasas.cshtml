@model StatistinesAtaskaitos.Models.ImoneGridModel[]
<table>
    <tr>
        <th></th>
        <th>ImoneGridModels kodas</th>
        <th>Pavadinimas</th>
    </tr>
            
    @for (var i = 0; i < Model.Length; i++ )
    {
        <tr>
            <td>
                <input type="checkbox" name="imoniuSarasas" value="@Model[i].ImoneGridModelsKodas" />
            </td>
            <td>@Model[i].ImoneGridModelsKodas</td>
            <td>@Model[i].ImoneGridModelsPavadinimas</td>
        </tr>
    }
</table>