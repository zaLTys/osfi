@using MvcContrib.UI.Grid
@model IEnumerable<StatistinesAtaskaitos.Models.Forma6>
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
    ViewBag.Title = "Index";
}
    <table>
        <tr>
            <th>Pavadinimas</th>
            <th>Kodas</th>
            <th>Vidutinis gyvuliu skaicius</th>
            <th>Produkcijos Kiekis</th>
            <th>Darbo apmokėjimas</th>
            <th>Pasarai</th>
            <th>Naftai</th>
            <th>Elektrai</th>
            <th>Kitos Islaidos</th>
            <th>Visos Islaidos</th>
            <th>Islaidos Pagrindinei Produkcijai</th>
            <th>Produkcijos savikaina</th>
        </tr>
        @foreach(var imones6 in Model)
        {
            <tr>   
                <td>@imones6.Pavadinimas</td>
                <td>@imones6.Kodas</td>
                <td>@Html.DisplayFor(x => imones6.VidutinisGyvuliuSk)</td>
                <td>@Html.DisplayFor(x => imones6.ProdukcijosKiekis)</td>
                <td>@Html.DisplayFor(x => imones6.IslaidosDarboApmokejimas)</td>
                <td>@Html.DisplayFor(x => imones6.IslaidosPasarai)</td>
                <td>@Html.DisplayFor(x => imones6.IslaidosNafta)</td>
                <td>@Html.DisplayFor(x => imones6.IslaidosElektra)</td>
                <td>@Html.DisplayFor(x => imones6.IslaidosKitos)</td>
                <td>@Html.DisplayFor(x => imones6.IslaidosVisos)</td>
                <td>@Html.DisplayFor(x => imones6.IslaidosPagrindinei)</td>
                <td>@Html.DisplayFor(x => imones6.ProdukcijosSavikaina)</td>


            </tr>       
        }
    </table>